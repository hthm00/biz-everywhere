<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
  <!--Navbar-->
  <div class="container-fluid">
    <div class="row">
        <header class="col-12 p-3 bg-dark text-light sticky-top pb-5" style="background-image: url('photos/Top Background.jpg'); background-size: cover;">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-6 col-lg-2">
                        <!-- Business Logo -->
                        <div class="BusinessLogo">
                            <h1 class="events text-center">
                                <img src="photos\BizEverywhereSign.jpg" class="img-fluid" alt="Business Logo" width="50%">
                            </h1>
                        </div>
                    </div>
                    <div class="col-6 col-lg-10 text-end">
                        <ul class="nav justify-content-end">
                            <li><a href="https://www.youtube.com/watch?v=dQw4w9WgXcQ&ab_channel=RickAstley" class="nav-link px-2 text-primary">Home</a></li>
                            <li><a href="#" class="nav-link px-2 text-secondary">Buy</a></li>
                            <li><a href="#" class="nav-link px-2 text-secondary">Sell</a></li>
                            <li><a href="#" class="nav-link px-2 text-secondary">About Us</a></li>
                        </ul>
                    </div>
                </div>           
                
                <!-- Block of text between navbar and search bar-->
                <div class="row align-items-center">
                    <div class="col-12 col-lg-6 offset-lg-3">
                        <form class="mb-3 mb-lg-0">
                            <div class="container my-3">
                                <div class="row">
                                    <div class="col-12">
                                    <p class="lead display-4">Discover Your Space To Grow Your Business</p>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                
                <!-- Search Bar -->
                <div class="row align-items-center">
                    <div class="col-12 col-lg-6 offset-lg-3">
                        <form class="mb-3 mb-lg-0">
                            <div class="input-group">
                                <input type="search" class="form-control form-control-dark" placeholder="Enter your zipcode..." aria-label="Search">
                                <button class="btn btn-outline-light" type="button">Search</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </header>
    </div>
  </div>
</head>
<!-- Body -->
<body class="d-flex flex-column min-vh-100">
  <div class="p-4 container">
      <div class="row">
          <div class="col-6">
              <div class="d-flex align-items-center">
                  <h3 class="text-decoration-none text-secondary">Recommended For You</h3>
              </div>
              <p>Stay informed of new listings.</p>
          </div>
          <div class="col-6">
              <nav class="navbar navbar-expand-sm navbar-dark">
                  <div class="container">
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                      <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
                      <form class="form-inline">
                        <div class="input-group">
                          <input type="email" class="form-control" placeholder="email@address.com">
                          <div class="input-group-append">
                            <button class="btn btn-primary" type="submit">Subscribe</button>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
              </nav>                      
          </div>
      </div>
  </div>
  <div class="container flex-grow-1">
      <!-- Columns in the body -->
      <div class="container">
          <div class="row">
              <div class="col-sm-4">
                  <div class="card text-bg-light">
                      <img src="photos\Y's Boba Shop.jpg" class="img-fluid" alt="Example image">
                      <div class="card-body">
                          <a href="#" class="stretched-link"></a>
                          <div class="row align-items-center text-left">
                              <div class="col-12">
                                  <div class="d-flex align-items-center">
                                      <h3 class="text-decoration-none text-secondary">Y's Boba Shop</h3>
                                      <button class="btn btn-outline-primary ml-auto"><i class="bi bi-heart"></i></button>
                                  </div>
                                  <a class="text-decoration-none text-secondary">1234 This St, This City, TS 01010</a>
                                  <br><a class="text-decoration-none text-primary">$732,000</a>
                              </div>                                              
                          </div>                              
                      </div>
                  </div>
              </div>                        
              <div class="col-sm-4">
                  <div class="card text-bg-light">
                      <img src="photos\Y's Building.jpg" class="img-fluid" alt="Example image">
                      <div class="card-body">
                          <a href="#" class="stretched-link"></a>
                              <div class="row align-items-center text-left">
                                  <div class="col-12">
                                      <div class="d-flex align-items-center">
                                          <h3 class="text-decoration-none text-secondary">Y's Commercial Building</h3>
                                          <button class="btn btn-outline-danger ml-auto"><i class="bi bi-heart"></i></button>
                                      </div>
                                      <a class="text-decoration-none text-secondary">4321 This St, This City, TS 10101</a>
                                      <br><a class="text-decoration-none text-primary">$1,370,000</a>
                                  </div>                                              
                              </div>                                                                                  
                          </a>  
                      </div>                                 
                  </div>
              </div>
              <div class="col-sm-4">
                  <div class="card text-bg-light">
                      <img src="photos\Y's Rental Office Space.jpg" class="img-fluid" alt="Example image">
                      <div class="card-body">
                          <a href="#" class="stretched-link"></a>
                              <div class="row align-items-center text-left">
                                  <div class="col-12">
                                      <div class="d-flex align-items-center">
                                          <h3 class="text-decoration-none text-secondary">Y's Rental Office Space</h3>
                                          <button class="btn btn-outline-danger ml-auto"><i class="bi bi-heart"></i></button>
                                      </div>
                                      <a class="text-decoration-none text-secondary">1324 This St, This City, TS 00100</a>
                                      <br><a class="text-decoration-none text-primary">$80,085</a>
                                  </div>                                              
                              </div>                                                                                  
                          </a>  
                      </div>                                 
                  </div>
              </div>
          </div>
      </div>
  </div>
  <footer>
    <%- include('../partials/footer'); %>
  </footer>
</body>
</html>